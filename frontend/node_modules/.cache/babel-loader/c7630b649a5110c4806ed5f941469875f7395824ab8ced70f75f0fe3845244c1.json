{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport UserManagment from '../services/signUp';\nimport SubjectManagement from '../services/subjects';\nexport default {\n  data() {\n    return {\n      user: {\n        id: '',\n        name: '',\n        surnames: '',\n        username: '',\n        role: 0\n      },\n      userDataLoaded: false,\n      subjects: []\n    };\n  },\n  created() {\n    const accessToken = this.$route.params.token;\n    if (accessToken) {\n      this.loadUserData(accessToken);\n    }\n  },\n  methods: {\n    async loadUserData(accessToken) {\n      try {\n        const userData = await UserManagment.loadUserData(accessToken);\n        if (userData.status != 404) {\n          this.user.id = userData.data.user_data.id;\n          this.user.name = userData.data.user_data.name;\n          this.user.surnames = userData.data.user_data.surnames;\n          this.user.username = userData.data.user_data.username;\n          this.user.role = userData.data.user_data.role;\n          try {\n            const userSubjects = await SubjectManagement.getUserSubjects(userData.data.user_data.id, userData.data.user_data.role);\n            this.subjects = userSubjects.data.subjects;\n            console.log('Asignaturas', userSubjects.data.subjects);\n            if (userSubjects.status === 200) {\n              this.userDataLoaded = true;\n              this.$router.push(`dashboard`);\n            }\n          } catch (error) {\n            this.userDataLoaded = true;\n            this.$router.push(`choose-subjects`);\n          }\n        } else {\n          console.log('User not found');\n          this.$router.push(`/`);\n        }\n      } catch (error) {\n        console.log('Error: ', error);\n        this.$router.push(`/`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["UserManagment","SubjectManagement","data","user","id","name","surnames","username","role","userDataLoaded","subjects","created","accessToken","$route","params","token","loadUserData","methods","userData","status","user_data","userSubjects","getUserSubjects","console","log","$router","push","error"],"sources":["C:\\Users\\david\\Desktop\\BISITE\\ProyectoAroa\\frontend\\src\\components\\UserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"header\">\r\n    <div class=\"logo\">\r\n      <router-link \r\n        :to=\"`/`\"\r\n        class=\"logo-link\">\r\n            David Garc√≠a\r\n        </router-link>\r\n        \r\n    </div>\r\n    <div class=\"menu\">\r\n      <div class=\"user-details\">\r\n        <h1>{{ user.name }} {{ user.surnames }}</h1>\r\n        <p>{{ user.username }}</p>\r\n      </div>\r\n\r\n      <div class=\"user-avatar\">\r\n        <img src=\"../assets/user_icon.png\">\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import UserManagment from '../services/signUp'\r\n  import SubjectManagement from '../services/subjects'\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        user: {\r\n          id: '',\r\n          name: '',\r\n          surnames: '',\r\n          username: '',\r\n          role: 0\r\n        },\r\n        userDataLoaded: false,\r\n        subjects: []\r\n      };\r\n    },\r\n    created() {\r\n      const accessToken = this.$route.params.token;\r\n      if (accessToken) {\r\n        this.loadUserData(accessToken)\r\n      }\r\n    },\r\n    methods: {\r\n      async loadUserData(accessToken) {\r\n          try {\r\n              const userData = await UserManagment.loadUserData(accessToken);\r\n  \r\n              if (userData.status != 404){\r\n                  this.user.id = userData.data.user_data.id;\r\n                  this.user.name = userData.data.user_data.name;\r\n                  this.user.surnames = userData.data.user_data.surnames;\r\n                  this.user.username = userData.data.user_data.username;\r\n                  this.user.role = userData.data.user_data.role;\r\n                  \r\n                 try {\r\n                      const userSubjects = await SubjectManagement.getUserSubjects(userData.data.user_data.id, userData.data.user_data.role);\r\n                      this.subjects = userSubjects.data.subjects\r\n                      console.log('Asignaturas', userSubjects.data.subjects)\r\n                      if(userSubjects.status === 200){\r\n                        this.userDataLoaded = true;\r\n                        this.$router.push(`dashboard`);\r\n                      }\r\n                 } catch (error) {\r\n                  this.userDataLoaded = true;\r\n                  this.$router.push(`choose-subjects`);\r\n                 }\r\n              } else {\r\n                  console.log('User not found');\r\n                  this.$router.push(`/`);\r\n              }\r\n          } catch (error){\r\n              console.log('Error: ', error);\r\n              this.$router.push(`/`);\r\n          }\r\n        }\r\n      }\r\n  }\r\n  </script>\r\n\r\n<style scoped>\r\n@import url(https://fonts.googleapis.com/css?family=Dosis:200,300,400,500,600,700,800);\r\n\r\n.menu {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: right;\r\n  margin-right: 30px;\r\n}\r\n\r\n.user-avatar {\r\n  width: 70px;\r\n  height: 70px;\r\n  background-color: #ccc;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  margin-top: 10px;\r\n  padding: 3px;\r\n}\r\n\r\nimg{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: contain;\r\n}\r\n\r\n.user-details {\r\n  margin-right: 20px;\r\n  margin-top: 15px;\r\n}\r\n\r\n.user-details h1 {\r\n  margin: 0;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.user-details p {\r\n  margin: 0;\r\n  color: #777;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n.header {\r\n    font-family: 'Dosis';\r\n  font-weig