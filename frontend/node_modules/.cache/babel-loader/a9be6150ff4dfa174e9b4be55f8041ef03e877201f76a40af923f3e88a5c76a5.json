      this.subjects = userSubjects.data.subjects\r\n                      console.log('Asignaturas', userSubjects.data.subjects)\r\n                      if(userSubjects.status === 200){\r\n                        this.userDataLoaded = true;\r\n                        this.$router.push(`dashboard`);\r\n                      }\r\n                 } catch (error) {\r\n                  this.userDataLoaded = true;\r\n                  this.$router.push(`choose-subjects`);\r\n                 }\r\n              } else {\r\n                  console.log('User not found');\r\n                  this.$router.push(`/`);\r\n              }\r\n          } catch (error){\r\n              console.log('Error: ', error);\r\n              this.$router.push(`/`);\r\n          }\r\n        }\r\n      }\r\n  }\r\n  </script>\r\n\r\n<style scoped>\r\n@import url(https://fonts.googleapis.com/css?family=Dosis:200,300,400,500,600,700,800);\r\n\r\n.menu {\r\n  display: flex;\r\n  flex-direction: row;\r\n  \r\n}\r\n\r\n.user-avatar {\r\n  width: 70px;\r\n  height: 70px;\r\n  background-color: #ccc;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n}\r\n\r\nimg{\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.user-details {\r\n  flex-grow: 1;\r\n  margin-left: 10px;\r\n}\r\n\r\n.user-details h1 {\r\n  margin: 0;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.user-details p {\r\n  margin: 0;\r\n  color: #777;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n.header {\r\n    font-family: 'Dosis';\r\n  font-weight: 300;\r\n  width: 100%;\r\n  border-bottom: 1px solid white;\r\n  color: white;\r\n  background-color: #0F0F0F;\r\n  text-align: right;\r\n  position: relative;\r\n  height: 90px;\r\n}\r\n  \r\n.header .logo {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    font-size: 32px;\r\n    font-weight: 200;\r\n    padding: 26px;\r\n    border-right: 1px solid white;\r\n  }\r\n\r\n.logo-link {\r\n  text-decoration: none;\r\n  color: white;\r\n}\r\n\r\n.link {\r\n  display: inline-block;\r\n  padding: 15px 0;\r\n  margin: 15px 25px;\r\n  position: relative;\r\n  text-decoration: none;\r\n  text-transform: uppercase;\r\n  color: white;\r\n  font-weight: 800;\r\n  font-size: 24px;\r\n}\r\n\r\n.link .bar {\r\n    width: 100%;\r\n    display: inline-block;\r\n    border-bottom: 5px solid white;\r\n    position: absolute;\r\n    bottom: 0;\r\n    left: 0;\r\n    opacity: 0;\r\n    transition: all 300ms;\r\n  }\r\n  \r\n.link:hover .bar {\r\n    opacity: 1;\r\n    bottom: 5px;\r\n}\r\n\r\n</style>\r\n"],"mappings":";OAYaA,UAA6B;;;EAXnCC,KAAK,EAAC;AAAQ;;EACZA,KAAK,EAAC;AAAM;;EAQZA,KAAK,EAAC;AAAM;gEACfC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAa,I,aACtBC,mBAAA,CAAmC;EAA9BC,GAA6B,EAA7BH;AAA6B,G;;EAG/BC,KAAK,EAAC;AAAc;;;uBAd7BG,mBAAA,CAmBM,OAnBNC,UAmBM,GAlBJH,mBAAA,CAOM,OAPNI,UAOM,GANJC,YAAA,CAIgBC,sBAAA;IAHbC,EAAE,EAAE,GAAG;IACRR,KAAK,EAAC;;sBAAY,MAElB,C,iBAFkB,gBAElB,E;;QAGJC,mBAAA,CASM,OATNQ,UASM,GARJC,UAEM,EAENT,mBAAA,CAGM,OAHNU,UAGM,GAFJV,mBAAA,CAA4C,YAAAW,gBAAA,CAArCC,KAAA,CAAAC,IAAI,CAACC,IAAI,IAAG,GAAC,GAAAH,gBAAA,CAAGC,KAAA,CAAAC,IAAI,CAACE,QAAQ,kBACpCf,mBAAA,CAA0B,WAAAW,gBAAA,CAApBC,KAAA,CAAAC,IAAI,CAACG,QAAQ,iB"},"metadata":{},"sourceType":"module","externalDependencies":[]}