r\n    return {\r\n      user: {\r\n        id: '',\r\n        name: '',\r\n        surnames: '',\r\n        username: ''\r\n      },\r\n      userDataLoaded: false,\r\n      subjects[{}]\r\n    };\r\n  },\r\n  created() {\r\n    const accessToken = this.$route.params.token;\r\n    if (accessToken) {\r\n      this.loadUserData(accessToken)\r\n    }\r\n  },\r\n  methods: {\r\n    async loadUserData(accessToken) {\r\n        try {\r\n            const userData = await UserManagment.loadUserData(accessToken);\r\n\r\n            if (userData.status != 404){\r\n                this.user.id = userData.data.user_data.id;\r\n                this.user.name = userData.data.user_data.name;\r\n                this.user.surnames = userData.data.user_data.surnames;\r\n                this.user.username = userData.data.user_data.username;\r\n                \r\n               try {\r\n                    const userSubjects = await SubjectManagement.getUserSubjects(userData.data.user_data.id, userData.data.user_data.role);\r\n                    if(userSubjects.status === 200){\r\n                      console.log('aquí toy')\r\n                      this.userDataLoaded = true;\r\n                      this.$router.push(`dashboard`);\r\n                    }\r\n               } catch (error) {\r\n                this.userDataLoaded = true;\r\n                this.$router.push(`choose-subjects`);\r\n               }\r\n            } else {\r\n                console.log('User not found');\r\n                this.$router.push(`/`);\r\n            }\r\n        } catch (error){\r\n            console.log('Error: ', error);\r\n            this.$router.push(`/`);\r\n        }\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.main-container {\r\n  max-width: 800px;\r\n  margin: auto;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.user-avatar {\r\n  width: 70px;\r\n  height: 70px;\r\n  background-color: #ccc;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n}\r\n\r\nimg{\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.user-details {\r\n  flex-grow: 1;\r\n  margin-left: 10px;\r\n}\r\n\r\n.user-details h1 {\r\n  margin: 0;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.user-details p {\r\n  margin: 0;\r\n  color: #777;\r\n}\r\n\r\n.dropdown {\r\n  position: relative;\r\n}\r\n\r\n.dropdown-button {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.dropdown-content {\r\n  position: absolute;\r\n  top: 100%;\r\n  right: 0;\r\n  background-color: #fff;\r\n  box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);\r\n  z-index: 1;\r\n  display: none;\r\n}\r\n\r\n.dropdown:hover .dropdown-content {\r\n  display: block;\r\n}\r\n\r\n.dropdown-content a {\r\n  color: black;\r\n  padding: 10px;\r\n  text-decoration: none;\r\n  display: block;\r\n  text-align: right;\r\n}\r\n\r\n.dropdown-content a:hover {\r\n  background-color: #f1f1f1;\r\n}\r\n\r\n.content {\r\n  /* Estilos para el contenido principal de la página */\r\n}\r\n</style>"],"mappings":";OAIaA,UAA6B;;;EAHnCC,KAAK,EAAC;AAAgB;;EACpBA,KAAK,EAAC;AAAW;gEACpBC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAa,I,aACtBC,mBAAA,CAAmC;EAA9BC,GAA6B,EAA7BH;AAA6B,G;;EAE/BC,KAAK,EAAC;AAAc;;gEAe3BC,mBAAA,CAEM;EAFDD,KAAK,EAAC;AAAS,I,aAClBG,mBAAA,6DAAgE,C;;;;6DArBpEF,mBAAA,CAuBM,OAvBNG,UAuBM,GAtBJH,mBAAA,CAkBM,OAlBNI,UAkBM,GAjBJC,UAEM,EACNL,mBAAA,CAGM,OAHNM,UAGM,GAFJN,mBAAA,CAA4C,YAAAO,gBAAA,CAArCC,IAAA,CAAAC,IAAI,CAACC,IAAI,IAAG,GAAC,GAAAH,gBAAA,CAAGC,IAAA,CAAAC,IAAI,CAACE,QAAQ,kBACpCX,mBAAA,CAA0B,WAAAO,gBAAA,CAApBC,IAAA,CAAAC,IAAI,CAACG,QAAQ,iB,GAErBC,UASM,C,GAERC,UAEM,C,GAEWN,IAAA,CAAAO,cAAc,I,cAAjCC,YAAA,CAA2FC,sBAAA;;IAAvDC,QAAQ,EAAEV,IAAA,CAAAC,IAAI;IAAGU,YAAY,EAAEX,IAAA,CAAAY"},"metadata":{},"sourceType":"module","externalDependencies":[]}