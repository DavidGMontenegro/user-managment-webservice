{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport UserManagment from '../services/signUp';\nimport SubjectManagement from '../services/subjects';\nexport default {\n  props: [userData],\n  data() {\n    return {\n      user: {\n        name: '',\n        surnames: '',\n        username: ''\n      }\n    };\n  },\n  created() {\n    const accessToken = this.$route.params.token;\n    if (accessToken) {\n      this.loadUserData(accessToken);\n    }\n  },\n  methods: {\n    async loadUserData(accessToken) {\n      try {\n        const userData = await UserManagment.loadUserData(accessToken);\n        if (userData.status != 404) {\n          console.log(userData.data.user_data);\n          this.user.name = userData.data.user_data.name;\n          this.user.surnames = userData.data.user_data.surnames;\n          this.user.username = userData.data.user_data.username;\n          try {\n            const userSubjects = await SubjectManagement.getUserSubjects(userData.data.user_data.id, userData.data.user_data.role);\n            if (userSubjects.status === 200) {\n              this.$router.push(`/`);\n            }\n          } catch (error) {\n            this.$router.push(`choose-subjects`);\n          }\n        } else {\n          console.log('User not found');\n          this.$router.push(`/`);\n        }\n      } catch (error) {\n        console.log('User not found');\n        this.$router.push(`/`);\n      }\n    }\n  }\n};","map":{"version":3,"names":["UserManagment","SubjectManagement","props","userData","data","user","name","surnames","username","created","accessToken","$route","params","token","loadUserData","methods","status","console","log","user_data","userSubjects","getUserSubjects","id","role","$router","push","error"],"sources":["C:\\Users\\david\\Desktop\\BISITE\\ProyectoAroa\\frontend\\src\\components\\UserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-container\">\r\n    <div class=\"user-info\">\r\n      <div class=\"user-avatar\">\r\n        <img src=\"../assets/user_icon.png\">\r\n      </div>\r\n      <div class=\"user-details\">\r\n        <h1>{{ user.name }} {{ user.surnames }}</h1>\r\n        <p>{{ user.username }}</p>\r\n      </div>\r\n      <div class=\"dropdown\">\r\n        <button class=\"dropdown-button\">\r\n          <i class=\"fas fa-chevron-down\"></i>\r\n        </button>\r\n        <div class=\"dropdown-content\">\r\n          <a href=\"#\">Profile</a>\r\n          <a href=\"#\">Settings</a>\r\n          <a href=\"#\">Logout</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"content\">\r\n      <!-- Aquí puedes agregar el contenido principal de la página -->\r\n    </div>\r\n  </div>\r\n  <router-view></router-view>\r\n</template>\r\n\r\n<script>\r\nimport UserManagment from '../services/signUp'\r\nimport SubjectManagement from '../services/subjects'\r\n\r\n\r\nexport default {\r\n  props: [userData],\r\n  data() {\r\n    return {\r\n      user: {\r\n        name: '',\r\n        surnames: '',\r\n        username: ''\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    const accessToken = this.$route.params.token;\r\n    if (accessToken) {\r\n      this.loadUserData(accessToken)\r\n    }\r\n  },\r\n  methods: {\r\n    async loadUserData(accessToken) {\r\n        try {\r\n            const userData = await UserManagment.loadUserData(accessToken);\r\n\r\n            if (userData.status != 404){\r\n                console.log(userData.data.user_data);\r\n                this.user.name = userData.data.user_data.name;\r\n                this.user.surnames = userData.data.user_data.surnames;\r\n                this.user.username = userData.data.user_data.username;\r\n                \r\n               try {\r\n                    const userSubjects = await SubjectManagement.getUserSubjects(userData.data.user_data.id, userData.data.user_data.role);\r\n                    if(userSubjects.status === 200){\r\n                        this.$router.push(`/`);\r\n                    }\r\n               } catch (error) {\r\n                        this.$router.push(`choose-subjects`);\r\n               }\r\n            } else {\r\n                console.log('User not found');\r\n                this.$router.push(`/`);\r\n            }\r\n        } catch (error){\r\n            console.log('User not found');\r\n            this.$router.push(`/`);\r\n        }\r\n      }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.main-container {\r\n  max-width: 800px;\r\n  margin: auto;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: 100%;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.user-avatar {\r\n  width: 70px;\r\n  height: 70px;\r\n  background-color: #ccc;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n}\r\n\r\nimg{\r\n    max-width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.user-details {\r\n  flex-grow: 1;\r\n  margin-left: 10px;\r\n}\r\n\r\n.user-details h1 {\r\n  margin: 0;\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n}\r\n\r\n.user-details p {\r\n  margin: 0;\r\n  color: #777;\r\n}\r\n\r\n.dropdown {\r\n  position: relative;\r\n}\r\n\r\n.dropdown-button {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.dropdown-content {\r\n  position: absolute;\r\n  top: 100%;\r\n  right: 0;\r\n  background-color: #fff;\r\n  box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);\r\n  z-index: 1;\r\n  display: none;\r\n}\r\n\r\n.dropdown:hover .dropdown-content {\r\n  display: block;\r\n}\r\n\r\n.dropdown-content a {\r\n  color: black;\r\n  padding: 10px;\r\n  text-decoration: none;\r\n  display: block;\r\n  text-align: right;\r\n}\r\n\r\n.dropdown-content a:hover {\r\n  background-color: #f1f1f1;\r\n}\r\n\r\n.content {\r\n  /* Estilos para el contenido principal de la página */\r\n}\r\n</style>"],"mappings":";AA6BA,OAAOA,aAAY,MAAO,oBAAm